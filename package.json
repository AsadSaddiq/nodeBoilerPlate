{
  "name": "poker-be",
  "version": "1.0.0",
  "description": "Poker Backend Application",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
